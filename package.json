{
  "name": "tpp-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "start": "node ./dist/server/entry.mjs",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop tpp-astro",
    "pm2:restart": "pm2 restart tpp-astro",
    "pm2:delete": "pm2 delete tpp-astro",
    "pm2:logs": "pm2 logs tpp-astro",
    "pm2:status": "pm2 status",
    "fetch:prod": "node scripts/fetch-production.mjs",
    "assets:download": "node scripts/download-assets.mjs",
    "parity:scan": "node scripts/parity-scan.mjs",
    "parity": "npm run fetch:prod && npm run assets:download && npm run build && npm run parity:scan",
    "deploy": "npm run build && npx wrangler pages deploy dist --project-name=tpp",
    "deploy:auto": "npm run parity && npx wrangler pages deploy dist --project-name=tpp"
  },
  "dependencies": {
    "@astrojs/cloudflare": "12.6.9",
    "@astrojs/node": "9.4.4",
    "astro": "^5.14.1",
    "cheerio": "1.1.2",
    "undici": "6.21.3"
  },
  "devDependencies": {
    "playwright": "1.55.0"
  }
}
