{
  "name": "tpp-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host --port 3001",
    "build": "astro build && node scripts/add-trailing-slashes.mjs",
    "preview": "astro preview",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:auto": "npm run parity && npm run deploy",
    "parity": "npm run fetch:prod && npm run assets:download && npm run build && npm run parity:scan",
    "fetch:prod": "node scripts/fetch-production.mjs",
    "assets:download": "node scripts/download-assets.mjs",
    "parity:scan": "node scripts/parity-scan.mjs",
    "blog:create": "echo 'Copy src/content/blog/TEMPLATE.md to new file - see MANUAL-CONTENT-PROCESS.md'",
    "blog:check": "./scripts/core-test.sh",
    "blog:link-map": "node automation/scripts/build-internal-link-map.mjs",
    "topics:generate": "node automation/scripts/generate-topics.mjs",
    "topics:check": "node automation/scripts/check-topic-queue.mjs",
    "test:essential": "./scripts/essential-tests.sh",
    "test:core": "./scripts/core-test.sh",
    "test": "playwright test",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:run": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:core:pw": "playwright test tests/core/",
    "test:mobile": "playwright test tests/mobile/",
    "test:pages": "playwright test tests/pages/",
    "test:blog": "playwright test tests/blog/",
    "test:report": "playwright show-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,astro,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,cjs,astro,md,json}\"",
    "image:check": "node scripts/check-image-dimensions.mjs",
    "image:check-all": "node scripts/check-image-dimensions.mjs *.png *.jpg *.jpeg *.webp 2>/dev/null || echo 'No images found'",
    "image:check-duplicates": "node automation/scripts/check-duplicate-images.mjs",
    "image:fix-duplicates": "node automation/scripts/fix-duplicate-images.mjs",
    "image:update-all": "node automation/scripts/update-blog-images.mjs",
    "image:refresh-all": "node automation/scripts/refresh-all-images.mjs",
    "health": "./scripts/simple-health-check.sh",
    "health:check": "./scripts/simple-health-check.sh",
    "health:setup": "./scripts/setup-health-monitoring.sh",
    "health:quick-setup": "./scripts/quick-setup.sh",
    "health:test": "./scripts/test-automation-system.sh",
    "status": "./scripts/simple-health-check.sh",
    "test:automation": "./scripts/test-automation-system.sh",
    "automation:suburb-pages": "node automation/scripts/generate-suburb-pages.mjs",
    "automation:gbp-posts": "node automation/scripts/gbp-auto-poster.mjs",
    "automation:gmb-export-buffer": "node automation/scripts/export-to-buffer-csv.mjs",
    "automation:gmb-send-makecom": "node automation/scripts/send-to-makecom.mjs",
    "automation:gmb-test-makecom": "node automation/scripts/send-to-makecom.mjs --test",
    "automation:gmb-send-n8n": "node automation/scripts/send-to-n8n.mjs",
    "automation:gmb-test-n8n": "node automation/scripts/send-to-n8n.mjs --test",
    "automation:reviews": "node automation/scripts/review-automation.mjs",
    "automation:rank-track": "node automation/scripts/rank-tracker.mjs",
    "automation:link-outreach": "node automation/scripts/link-outreach.mjs",
    "automation:generate-topics": "node automation/scripts/generate-topics.mjs",
    "automation:dashboard": "node automation/scripts/dashboard.mjs",
    "automation:dashboard:watch": "node automation/scripts/dashboard.mjs --watch",
    "automation:scheduled": "node automation/scripts/automation-orchestrator.mjs scheduled",
    "automation:run": "node automation/scripts/automation-orchestrator.mjs run",
    "automation:list": "node automation/scripts/automation-orchestrator.mjs list",
    "automation:status": "node automation/scripts/automation-orchestrator.mjs status",
    "automation:help": "node automation/scripts/automation-orchestrator.mjs help",
    "automation:setup": "bash automation/scripts/setup-automation.sh",
    "automation:test": "node automation/scripts/test-automation.mjs",
    "automation:health": "node automation/scripts/health-dashboard.mjs",
    "automation:monitor": "bash automation/scripts/vps-monitor.sh",
    "automation:wizard": "node automation/scripts/setup-wizard.mjs",
    "automation:status:quick": "node automation/scripts/quick-status.mjs",
    "automation:cost-estimate": "node automation/scripts/cost-calculator.mjs",
    "automation:backup": "bash automation/scripts/backup-system.sh",
    "automation:verify-env": "node automation/scripts/verify-environment.mjs",
    "automation:analyze-content": "node automation/scripts/analyze-content-quality.mjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@astrojs/cloudflare": "12.6.9",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@google/generative-ai": "^0.24.1",
    "@perplexity-ai/perplexity_ai": "^0.12.0",
    "astro": "^5.14.3",
    "cheerio": "1.1.2",
    "commander": "^14.0.1",
    "dataforseo-client": "^2.0.7",
    "dotenv": "^17.2.3",
    "google-ads-api": "^21.0.1",
    "google-search-results-nodejs": "^2.1.0",
    "googleapis": "^161.0.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.7",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitest/ui": "^4.0.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "vitest": "^4.0.5"
  }
}
