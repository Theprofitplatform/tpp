---
export interface Props {
  title?: string;
  description?: string;
  canonical?: string;
  ogTitle?: string;
  ogDescription?: string;
  ogImage?: string;
  ogUrl?: string;
  twitterTitle?: string;
  twitterDescription?: string;
  twitterImage?: string;
  keywords?: string;
  noindex?: boolean;
}

const {
  title = "Index | The Profit Platform - Sydney Digital Marketing",
  description = "Leading Index services in Sydney. Get results with The Profit Platform. ✓ Proven ROI ✓ 127+ Reviews ✓ Free Consultation",
  canonical = "https://theprofitplatform.com.au/",
  ogTitle = "Index - The Profit Platform Sydney",
  ogDescription = "Transform your business with expert Index services",
  ogImage = "https://theprofitplatform.com.au/images/og-index.jpg",
  ogUrl = "https://theprofitplatform.com.au/index.html",
  twitterTitle = "Sydney Digital Marketing Expert | Fresh Approach, Proven Results",
  twitterDescription = "Sydney's hungry new digital marketing expert. Founded 2024 with fresh strategies & personalised service. 15+ clients already seeing 2x more leads. Serving Bondi, Parramatta, Penrith & Greater Sydney.",
  twitterImage = "https://theprofitplatform.com.au/wp-content/uploads/2025/08/The-Profit-Platform-Growth-Dashboard-1.png",
  keywords = "Index, sydney, digital marketing, seo, google ads, web design",
  noindex = false
