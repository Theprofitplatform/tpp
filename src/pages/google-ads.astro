            <!-- Counter Animation Script -->
            <script>
                // Animated counter function
                function animateCounter(id, start, end, duration, suffix = '') {
                    const element = document.getElementById(id);
                    if (!element) return;

                    let current = start;
                    const increment = (end - start) / (duration / 16);

                    const timer = setInterval(() => {
                        current += increment;
                        if ((increment > 0 && current >= end) || (increment < 0 && current <= end)) {
                            element.textContent = end.toLocaleString() + suffix;
                            clearInterval(timer);
                        } else {
                            element.textContent = Math.floor(current).toLocaleString() + suffix;
                        }
                    }, 16);
                }

                // Start animations when section is in view
                const observer = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            animateCounter('impressions-counter', 0, 2847392, 2000);
                            animateCounter('clicks-counter', 0, 48726, 2000);
                            animateCounter('ctr-counter', 0, 4.8, 2000);
                            animateCounter('conversions-counter', 0, 3284, 2000);
                            animateCounter('cpc-counter', 0, 2.45, 2000);
                            animateCounter('roas-counter', 0, 5.2, 2000);
                            observer.unobserve(entry.target);
                        }
                    });
                });

                const dashboardSection = document.querySelector('.performance-dashboard-section');
                if (dashboardSection) {
                    observer.observe(dashboardSection);
                }

                // Update "last update" timer
                let seconds = 2;
                setInterval(() => {
                    seconds++;
                    const updateElement = document.getElementById('last-update');
                    if (updateElement) {
                        if (seconds < 60) {
                            updateElement.textContent = seconds + ' seconds ago';
                        } else {
                            updateElement.textContent = Math.floor(seconds / 60) + ' minute' + (Math.floor(seconds / 60) > 1 ? 's' : '') + ' ago';
                        }
                    }
                }, 1000);
