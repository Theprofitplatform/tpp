---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/Header.astro';

const pageTitle = "Services | The Profit Platform - Sydney Digital Marketing";
const pageDescription = "Full-service digital marketing agency in Sydney. SEO, Google Ads, web design, social media marketing, and conversion optimization. Customized strategies that deliver real ROI.";
---

<BaseLayout title={pageTitle} description={pageDescription}>
  <link rel="stylesheet" href="/css/services-page-styles.css">

  <!-- Preconnect to external resources -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link rel="preconnect" href="https://cdnjs.cloudflare.com">

  <!-- SEO Meta Tags -->
  <meta name="description" content={pageDescription} slot="head">
  <meta name="robots" content="index, follow" slot="head">
  <meta name="author" content="The Profit Platform" slot="head">
  <link rel="canonical" href="https://theprofitplatform.com.au/services" slot="head">

  <!-- Open Graph -->
  <meta property="og:type" content="website" slot="head">
  <meta property="og:url" content="https://theprofitplatform.com.au/services" slot="head">
  <meta property="og:title" content="Services - The Profit Platform Sydney" slot="head">
  <meta property="og:description" content="Transform your business with expert Services services" slot="head">
  <meta property="og:image" content="https://theprofitplatform.com.au/images/og-services.jpg" slot="head">

  <!-- Twitter Card -->
  <meta property="twitter:card" content="summary_large_image" slot="head">
  <meta property="twitter:url" content="https://theprofitplatform.com.au/services" slot="head">
  <meta property="twitter:title" content="Digital Marketing Services Sydney | SEO, Google Ads & Web Design - The Profit Platform" slot="head">
  <meta property="twitter:description" content="Comprehensive digital marketing services in Sydney. Expert SEO, Google Ads management, web design, social media marketing, and analytics. Get 3x more leads with proven strategies." slot="head">
  <meta property="twitter:image" content="https://theprofitplatform.com.au/images/services-twitter-image.jpg" slot="head">

  <Header />

  <main id="main-content">
    <!-- Services Hero Section -->
    <section class="services-hero" id="services-hero">
      <div class="hero-background-elements">
        <div class="floating-shape shape-1"></div>
        <div class="floating-shape shape-2"></div>
        <div class="floating-shape shape-3"></div>
      </div>

      <div class="container">
        <div class="services-hero-content">
          <div class="hero-badge">
            <i class="fas fa-rocket"></i>
            <span>Complete Digital Marketing Solutions</span>
          </div>

          <h1 class="hero-title">
            <span class="highlight-text">Expert Digital Marketing</span><br>
            Services for <span class="location-text">Sydney</span> Businesses
            <div class="title-underline"></div>
          </h1>

          <p class="hero-subtitle">
            From SEO and Google Ads to web design and social media marketing, we provide comprehensive digital marketing solutions that drive real results for Sydney businesses.
          </p>

          <!-- Conversion Elements: Trust & Urgency -->
          <div class="conversion-elements">
            <div class="conversion-badge" style="background: rgba(16, 185, 129, 0.1); color: #059669; border: 1px solid rgba(16, 185, 129, 0.3);">
              <i class="fas fa-medal" style="color: #059669;"></i>
              Full-Service Agency
            </div>
            <div class="conversion-badge" style="background: rgba(59, 130, 246, 0.1); color: #1d4ed8; border: 1px solid rgba(59, 130, 246, 0.3);">
              <i class="fas fa-handshake" style="color: #1d4ed8;"></i>
              15+ Happy Clients
            </div>
            <div class="conversion-badge" style="background: rgba(245, 158, 11, 0.1); color: #d97706; border: 1px solid rgba(245, 158, 11, 0.3);">
              <i class="fas fa-gift" style="color: #d97706;"></i>
              Free Strategy Call
            </div>
          </div>

          <figure class="hero-stats">
            <div class="stat-item">
              <div class="stat-number">6</div>
              <div class="stat-label">Core Services</div>
            </div>
            <div class="stat-item">
              <div class="stat-number">20+</div>
              <div class="stat-label">Projects Completed</div>
            </div>
            <div class="stat-item">
